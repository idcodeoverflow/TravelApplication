<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="self.d3veloper.travelapplication" >

    <application
        android:allowBackup="true"
        android:name="cbedoy.cblibrary.services.ApplicationLoader"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >


        <meta-data android:name="DATABASE" android:value="travelapplication.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="elf.d3veloper.travelapplication" />

        <activity
            android:name=".MasterViewController"
            android:label="@string/app_name" >
        </activity>

        <activity android:name=".SplashViewController">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
